<%- partial('_partial/head') %>

<body class="<% if (page.source =="index.md") {%> <%= "bg-scroll" %> <%}%>">
<div id="page" class="site">
  <div id="primary" class="content-area">

    <% if (page.source!="index.md") {%>
      <header id="header" class="inner"><%- partial('_partial/header') %></header>
    <%}%>
    
    <article id="content" class="site-content">
      <main id="main" class="site-main posts-loop" role="main">
        <%- body %>
      </main>
    </article>

    <footer id="colophon" class="site-footer" role="contentinfo"><%- partial('_partial/footer') %></footer>
    <%- partial('_partial/googleanalytics') %>
  </div>
</div>
</body>
</html>